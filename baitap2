#include<stdio.h>

int main(){
    int n;
    int tachso;// tao bien de tach so = toan tu chia du
    int s = 0;// de tong cac so 
    printf("Nhap n: ");
    scanf("%d",&n);
    for(;n!=0;){// dat vao for de chay 
        tachso = n % 10;// chi so nguyen de lay so cuoi
        s += tachso;
        n /= 10;
    }    
    printf("%d",s);
    return 0;
}
